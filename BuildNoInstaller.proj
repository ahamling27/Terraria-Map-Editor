<?xml version="1.0" encoding="utf-8"?><Project ToolsVersion="4.0" DefaultTargets="Go" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">    <Import Project="$(MSBuildProjectDirectory)\DateStampVersion.tasks" />    <PropertyGroup>        <Configuration Condition="'$(Configuration)' == ''">Debug</Configuration>        <SolutionRoot>$(MSBuildProjectDirectory)</SolutionRoot>	    <BuildNumber Condition="'$(BuildNumber)' == ''" />    </PropertyGroup>    <ItemGroup>        <RegexTransformItems Include="**\TEditXna\Properties\AssemblyInfo.cs" />		<!--<RegexTransformItems Include="**\TEdit3Installer\Product.wxs" />-->    </ItemGroup>    <Target Name="Go" DependsOnTargets="UpdateAssemblyVersion; Build">    </Target>    <Target Name="UpdateAssemblyVersion" >        <RegexTransform Items="@(RegexTransformItems)" Vers="$(BuildNumber)" />    </Target>    <Target Name="Build">        <MSBuild Projects="$(SolutionRoot)\TEditNoInstaller.sln" Targets="Build" />    </Target></Project>